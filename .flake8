# .flake8
[flake8]
select = ANN,B,B9,BLK,C,D,E,F,I,W
ignore = E203,W503,ANN101,ANN102
per-file-ignores =
    tests/*:S101,ANN
    noxfile.py:ANN
max-line-length = 88
max-complexity = 10

docstring-convention = google

application-import-names = path_planning, tests
import-order-style = google

allow-star-arg-any = true
